{
  "name": "weatherapp",
  "version": "0.1.0",
  "description": "A simple weather application that uses the OpenWeather API to access local weather data",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=dev && npm run clean && npm run build && concurrently 'npm:watch-js' 'npm:server'",
    "watch-js": "webpack --env.dev --watch",
    "server": "tsnd src/server/app.ts",
    "build": "webpack --env.dev",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrBurtyyy/WeatherApp.git"
  },
  "keywords": [
    "open",
    "source",
    "weather",
    "application"
  ],
  "author": "Alex Burt",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/MrBurtyyy/WeatherApp/issues"
  },
  "homepage": "https://github.com/MrBurtyyy/WeatherApp#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.5",
    "axios": "^0.18.0",
    "express": "^4.16.4"
  }
}
